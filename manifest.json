{

    "manifest_version": 2,
    "name": "Start Fast Offer",
    "version": "1.0",

    "description": "",

    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "<all_urls>"
    ],

    "background": {
        "scripts": ["jquery.min.js", "background.js"]
    },

    "content_scripts": [
        {
            "matches": ["*://*.v3.torontomls.net/*"],
            "js": ["jquery.min.js", "main.js"]
        }
    ],

    "web_accessible_resources": [
        "icons/icon.png"
    ]
}
